material YellowSpark
{
	technique
	{
		pass
		{
			lighting off
			scene_blend alpha_blend
			depth_write off
			diffuse vertexcolour

			texture_unit
			{
				texture yellowSpark.png
				tex_address_mode clamp
			}
		}
	}
}
material OrangeSpark
